{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install && cp .env.example .env",
    "lint:script": "eslint --fix --ignore-pattern .gitignore .",
    "lint:prettier": "npx prettier \"**/*.{md,json,yml,yaml}\" --write",
    "lint:style": "stylelint --fix --config stylelint.config.cjs",
    "test": "vitest"
  },
  "lint-staged": {
    "*.{js,ts,vue,cjs,mjs}": "pnpm lint:script",
    "*.{md,json,yml,yaml}": "pnpm lint:prettier",
    "*.{vue,scss,css}": "pnpm lint:style"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "1.3.1",
    "@nuxt/image": "1.10.0",
    "@nuxt/test-utils": "3.18.0",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/sitemap": "7.2.10",
    "@nuxtjs/stylelint-module": "5.2.1",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@types/node": "22.16.0",
    "@unocss/reset": "66.3.2",
    "@vee-validate/nuxt": "4.15.0",
    "@vee-validate/zod": "4.15.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "@zadigetvoltaire/nuxt-gtm": "0.0.13",
    "dayjs": "1.11.13",
    "eslint": "9.26.0",
    "happy-dom": "17.6.3",
    "husky": "9.1.7",
    "nuxt": "3.17.2",
    "nuxt-gtag": "3.0.2",
    "ofetch": "1.4.1",
    "postcss-scss": "4.0.9",
    "sass": "1.89.2",
    "sass-loader": "16.0.5",
    "stylelint": "16.21.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-order": "6.0.4",
    "typescript": "5.8.3",
    "vee-validate": "4.15.1",
    "vitest": "3.2.4",
    "vue": "3.5.17",
    "vue-router": "4.5.1",
    "zhead": "2.2.4",
    "zod": "3.25.74"
  },
  "browserslist": [
    "> 2% in JP",
    "ios >= 15",
    "not dead"
  ],
  "packageManager": "pnpm@10.12.4"
}
